<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:layout_marginBottom="@dimen/eight_dp">    <ImageView        android:id="@+id/bicycle"        android:layout_width="40dp"        android:layout_height="40dp"        android:layout_marginStart="@dimen/twenty_four_dp"        android:layout_marginTop="@dimen/thirty_six_dp"        app:srcCompat="@drawable/ic_bicycle" />    <TextView        android:id="@+id/date_time"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="@dimen/thirty_dp"        android:layout_marginTop="@dimen/sixteen_dp"        android:layout_toRightOf="@id/bicycle"        android:padding="@dimen/two_dp"        android:text="16 March 2018"        android:textColor="@color/color_grey_02"        android:textSize="@dimen/fourteen_sp" />    <TextView        android:id="@+id/bicycle_id"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="@dimen/thirty_dp"        android:layout_marginTop="@dimen/fourty_dp"        android:layout_toRightOf="@id/bicycle"        android:padding="@dimen/two_dp"        android:text="WB213426G"        android:textColor="@color/color_black_01"        android:textSize="@dimen/sixteen_sp" />    <TextView        android:id="@+id/text_required"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@id/bicycle_id"        android:layout_marginBottom="@dimen/eight_dp"        android:layout_marginStart="@dimen/thirty_dp"        android:layout_marginTop="@dimen/two_dp"        android:layout_toRightOf="@id/bicycle"        android:padding="@dimen/two_dp"        android:text="Some text if required"        android:textColor="@color/color_grey_01"        android:textSize="@dimen/tweleve_sp" />    <TextView        android:id="@+id/time_stamp"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentEnd="true"        android:layout_marginEnd="@dimen/twenty_four_dp"        android:layout_marginTop="@dimen/twenty_four_dp"        android:padding="@dimen/two_dp"        android:text="20 mins"        android:textColor="@color/color_black_01"        android:textSize="@dimen/fourteen_sp" />    <TextView        android:id="@+id/money_tv"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentEnd="true"        android:layout_marginEnd="@dimen/fifty_dp"        android:layout_marginTop="@dimen/sixty_dp"        android:padding="@dimen/two_dp"        android:text="â‚¹"        android:textColor="@color/green_800" />    <TextView        android:id="@+id/money"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentEnd="true"        android:layout_marginEnd="@dimen/thirty_six_dp"        android:layout_marginTop="@dimen/sixty_dp"        android:padding="@dimen/four_dp"        android:text="5"        android:textColor="@color/green_800" /></RelativeLayout>